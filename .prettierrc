{
  "singleQuote": false,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.{js,ts,jsx,tsx,html,css,scss,sass}",
      "options": {
        "useTabs": true
      }
    },
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "useTabs": true
      }
    }
  ]
}
